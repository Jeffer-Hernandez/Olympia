<h1> Sign Up! </h1>

<%= form_for @user do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :age%>
    <%= f.number_field :age %>

    <%= f.label :height %>
    <%= f.text_field :height %>

    <%= f.label :weight %>
    <%= f.number_field :weight %>

    <%= f.submit "Sign Up" %>
    

<% end %>
