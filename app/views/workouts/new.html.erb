<h1> Create a New Workout</h1>

<%= form_for @workout do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    <br></br>

    <%= f.label :category %>
    <%= f.select :category,options_for_select(["legs", "shoulders", "biceps", "triceps", "back", "chest", "abs"]), include_blank: true  %>
    <br> </br>

    <%= f.label :intensity %>
    <%= f.text_field :intensity %>
    <br> </br>

    <%= f.submit "Create" %>

   
    

<% end %>