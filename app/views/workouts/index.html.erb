<h1>All Workouts</h1>

<ul>
<% @workouts.each do |workout| %>
<li>

  <strong><%= link_to workout.name, workout_path(workout)%></strong> -
  <%= link_to workout.category, workout_path(workout) %>  -
  <%= link_to workout.intensity, workout_path(workout) %> - added by: <%= workout.user.username %> 
  

</li>


<% end %>
</ul>
